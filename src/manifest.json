{
    "manifest_version": 2,
    "name": "ItSecScores",
    "version": "1.3.0",
    "description": "more precise scores for the scoreboard",
    "content_scripts": [
        {
            "matches": [
                "*://scoreboard.sec.in.tum.de/scoreboard"
            ],
            "js": [
                "scores.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "options_ui": {
        "page": "options.html"
    }
}